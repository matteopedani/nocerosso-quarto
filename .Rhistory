library(IRdisplay)
library(repr)
calculate <- function(salary, tax_rate, dividend, dividend_tax_rate) {
# Calcolare gli importi netti di stipendi e dividendi
net_salary <- salary * (1 - tax_rate/100)
net_dividend <- dividend * (1 - dividend_tax_rate/100)
# Visualizzare i risultati
salary_label$value <- paste("Stipendio netto:", format(net_salary, nsmall = 2))
dividend_label$value <- paste("Dividendo netto:", format(net_dividend, nsmall = 2))
}
# Creare i widget per le variabili
salary_slider <- sliderInput("salary", "Stipendio lordo:", min = 0, max = 10000, value = 5000)
